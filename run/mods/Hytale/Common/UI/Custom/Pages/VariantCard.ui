Button {
  Anchor: (Bottom: 5, Right: 5);
  Padding: (Horizontal: 10, Vertical: 6);
  Background: (Color: #2a2a2a(0.6));
  Style: (
    Hovered: (
      Background: #3a3a3a(0.8),
    ),
    Pressed: (
      Background: #4a4a4a(0.9),
    )
  );

  Label #VariantUsage {
    Style: (
      FontSize: 12,
      TextColor: #ffffff(0.9)
    );
  }
}

